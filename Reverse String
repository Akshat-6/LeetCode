class Solution {
    public void reverseString(char[] s) {
        String s1="";
        StringBuilder sb=new StringBuilder();
        for(char str:s){
            sb.append(str);
        }
        sb=sb.reverse();
        s1=sb.toString();
        for(int i=0;i<s1.length();i++){
            s[i]=s1.charAt(i);
        }
        System.out.println(Arrays.toString(s));

    }
}
