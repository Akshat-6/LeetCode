class Solution {
    public int getSum(int a, int b) {
        double base = 2.0;
        double expA = Math.pow(base, a / 10.0);
        double expB = Math.pow(base, b / 10.0);
        double logResult = Math.log(expA * expB) / Math.log(2);

        int result = (int)Math.round(10.0 * logResult);

        return result;
    }
}
